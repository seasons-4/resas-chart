{
  "name": "resas-chart",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:next dev:scss",
    "dev:next": "next dev",
    "dev:scss": "typed-scss-modules src --watch",
    "build": "next build",
    "start": "next start",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|json|ts|tsx)\"",
    "lint": "next lint && npm run stylelint",
    "lint:fix": "next lint --fix && npm run prettier && npm run stylelint:fix",
    "stylelint": "stylelint \"**/*.+(css|scss)\"",
    "stylelint:fix": "stylelint \"**/*.+(css|scss)\" --fix",
    "typegen:scss": "typed-scss-modules src",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test-storybook": "test-storybook",
    "test": "jest"
  },
  "dependencies": {
    "clsx": "^2.0.0",
    "lodash": "^4.17.21",
    "next": "14.0.3",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@storybook/addon-essentials": "^7.6.3",
    "@storybook/addon-interactions": "^7.6.3",
    "@storybook/addon-links": "^7.6.3",
    "@storybook/addon-viewport": "^7.6.3",
    "@storybook/blocks": "^7.6.3",
    "@storybook/jest": "^0.2.3",
    "@storybook/nextjs": "^7.6.3",
    "@storybook/react": "^7.6.3",
    "@storybook/test": "^7.6.3",
    "@storybook/test-runner": "^0.16.0",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.10",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "destyle.css": "^4.0.0",
    "eslint": "^8",
    "eslint-config-next": "14.0.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-storybook": "^0.6.15",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.0",
    "sass": "^1.69.5",
    "storybook": "^7.6.3",
    "stylelint": "^15.11.0",
    "stylelint-config-recess-order": "^4.4.0",
    "stylelint-config-recommended-scss": "^13.1.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typed-scss-modules": "^8.0.0",
    "typescript": "^5",
    "url-search-params-delete": "^0.2.0"
  },
  "volta": {
    "node": "20.2.0",
    "npm": "9.6.6"
  }
}
